---
import MegaNavKt from './MegaNavKt.astro';
import MegaNavGr from './MegaNavGr.astro';
import type { BaseProps } from './types';

interface Props extends BaseProps {}

const { env, page } = Astro.props;
---

{env.targetRegion === "US" ? (
  <MegaNavKt env={env} page={page} />
) : (
  <MegaNavGr env={env} page={page} />
)}
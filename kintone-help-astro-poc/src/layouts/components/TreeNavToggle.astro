<div id="treenav-toggle">
  <button
    id="treenav-show"
    class="treenav-toggle-button"
    title="サイドナビゲーションを表示する"
  >
    <i class="fas fa-chevron-right" aria-hidden="true"></i>
  </button>
  <button
    id="treenav-hide"
    class="treenav-toggle-button"
    title="サイドナビゲーションを表示しない"
  >
    <i class="fas fa-chevron-left" aria-hidden="true"></i>
  </button>
</div>
